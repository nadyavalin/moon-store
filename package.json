{
  "name": "moon-store",
  "version": "1.0.0",
  "description": "moon-store",
  "main": "webpack.base.config.js",
  "scripts": {
    "start": "webpack serve --open --config ./webpack.config.js --env mode=dev",
    "build": "webpack --config ./webpack.config.js --env mode=prod",
    "format": "prettier --write .",
    "ci:format": "prettier --check .",
    "lint": "eslint --ext .ts,.js",
    "lint:fix": "eslint --fix --ext .ts,.js",
    "prepare": "husky",
    "test": "jest --silent",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --silent",
    "test:watch": "jest --watch",
    "test:e2e": "npx playwright test",
    "coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "commit-message-validator": "^1.0.2",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "3.0.3",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.3.3",
    "url-loader": "^4.1.1",
    "validate-branch-name": "^1.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.0",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "external-svg-loader": "^1.7.1"
  },
  "config": {
    "commit-message-validator": {
      "pattern": "^MS-(XXX|\\d{1,5}) (build|ci|chore|docs|feat|fix|perf|refactor|revert|style|test) ?\\((\\*|.{1,20})\\): ",
      "errorMessage": "The commit message format is incorrect."
    }
  }
}
